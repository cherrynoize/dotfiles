#
# ~/.bash_aliases
#

alias -- -='cd ~'
alias bashrc='$EDITOR ~/.bashrc'
alias hw='echo hello, world!'
alias sudo='sudo '
alias sudo='sudo -v; sudo ' # Auto-refresh sudo privileged session.
alias im='sudo -iu' # Run interactive shell as user.
alias runas='sudo -u' # Run command as user.
alias open='xdg-open'
alias ls='ls --color=auto'
alias ll='ls -l'
alias la='ls -a'
alias l='ls'
alias e='echo'
alias vimconfig='$EDITOR ~/.vimrc'
alias nvim='launch-nvim'
alias xobfu='zzxorcopy' # Simple xor obfuscate.
alias ff='firefox'
#alias fni='firefox-nightly'
#alias fde='firefox-developer-edition'
alias chrome='google-chrome-stable'
